config section 'config'
    list cards 'systemCard'
    list cards 'networkEventsCard'
    list cards 'systemEventsCard'
    list cards 'interfaceCards'

config section 'visibility'
    option System '1'
    option Recent_network_events '0'
    option Recent_system_events '1'

config section 'order'
    option System '0'
    option Recent_network_events '1'
    option Recent_system_events '2'